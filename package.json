{
  "name": "electron-fcm-receiver-poc",
  "version": "1.0.0",
  "description": "A demo FCM client",
  "keywords": [
    "Electron",
    "FCM",
    "push messages"
  ],
  "scripts": {
    "start": "npm-run-all -p start:client start:server",
    "start:client": "npm --prefix client start",
    "start:server": "npm --prefix server start",
    "postinstall": "npm i --prefix client && npm i --prefix server"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
